            <div style="display:grid; grid-template-columns: repeat(auto-fill, minmax(280px, 1fr)); gap: 1rem;">
                {% for iconSet in iconSets %}
                    <div class="IconSetCard">
                        <h3 class="ubuntu-title">
                            <a class="stretched-link "
                                href="{{ path('app_icon_collection', {prefix: iconSet.identifier}) }}"
                               style="color: var(--color-text);"
                            >{{ iconSet.name }}</a>
                        </h3>
                        <span class="IconSetCard__total">{{ iconSet.total }}</span>
                        <div class="IconSetCard__icons">

                            {% for sample in iconSet.samples %}
                                <img src="https://api.iconify.design/{{ iconSet.identifier }}/{{ sample }}.svg" alt="{{ sample }}" style="width: 24px; height: 24px;">
                            {% endfor %}

                        </div>
                    </div>
                {% endfor %}
            </div>
