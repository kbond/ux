    </section>

    <section class="section">
        <div class="section-heading">
            AA
        </div>
        <p class="eyebrows" style="margin: 111px 0 0;">Iconic Packages</p>
        <div class="d-md-flex justify-content-md-between text-center align-items-center text-md-start">
            <h2 class="ubuntu pt-2 component-headlines">Popular <em class="rainbow-emphasis">Brand Sets</em></h2>
            <a style="height: 100%;" class="btn btn-md btn-outline-primary mt-3 mt-md-0" href="{{ url('app_icon_sets_brands') }}">
                Brand Icon Sets
                <twig:Icon name="arrow-right"  />
            </a>
        </div>
        <div class="mt-3 mt-md-5">
            <twig:Icon:IconSetGrid category="brand" samples="6" limit="10"  />
        </div>
    </section>

{% endblock %}


